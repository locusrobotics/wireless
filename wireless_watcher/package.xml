<?xml version="1.0"?>
<package format="2">
  <name>wireless_watcher</name>
  <version>0.4.0</version>
  <description>A Python-based which publishes connection information about a linux wireless interface.</description>

  <maintainer email="delkan@locusrobotics.com">David Elkan</maintainer>
  <author>Mike Purvis</author>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>roslint</build_depend>

  <exec_depend>ethtool</exec_depend>
  <exec_depend>rospy</exec_depend>
  <exec_depend>wireless_msgs</exec_depend>
  <exec_depend>pciutils</exec_depend>

  <test_depend>roslaunch</test_depend>
</package>
